<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - serkanbilsel</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/serkanbilsel.styles.css" asp-append-version="true" />
  
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="/xml.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.9.359/pdf.js"></script>
    <link href='https://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>
</head>
  <section class="wrapper">
  @*   <div id="stars"></div>
    <div id="stars2"></div>
    <div id="stars3"></div>  *@
</section>   
<body>
    <header>
        <section class="wrapper">
            <div id="title">
                <span></span>
                <br />
                <span class="enter">
                    
                    @*  <a href="///">enter backend</a> *@
                </span>
            </div>
        </section>
        

        <div class="header">
            <a href="" class="logo">SERKAN</a>
            <nav class="navbar">

               
                <a asp-action="Index" asp-controller="Portfolio">Portfolio</a>
                <a asp-action=" " asp-controller="">Contact</a>
                <header>
                    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
                        <div class="container-fluid">
                            <a class="navbar-brand" asp-area="Admin" asp-controller="Home" asp-action="Index">SerkanBilsel Admin</a>
                            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                                    aria-expanded="false" aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"></span>
                            </button>
                            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between ">
                                <ul class="navbar-nav flex-grow-1">
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="Admin" asp-controller="Main" asp-action="Index">Ana Ekran</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="Admin" asp-controller="Categories" asp-action="Index">Kategoriler</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="Admin" asp-controller="Products" asp-action="Index">Ürünler</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="Admin" asp-controller="Sliders" asp-action="Index">Slider Yönetimi</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="Admin" asp-controller="Contacts" asp-action="Index">İletişim Formları</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="Admin" asp-controller="Users" asp-action="Index">Kullanıcılar</a>
                                    </li>
                                </ul>
                            </div>
                            <a class="navbar-brand btn btn-warning" asp-area="Admin" asp-controller="Login" asp-action="Logout">Oturumu Kapat</a>
                        </div>
                    </nav>
                </header>
            </nav>
        </div>
      
    </header>
    <div class="">
        <main role="main">
            @RenderBody()
        </main>
    </div>

      <footer class="border-top footer text-muted"> 
     <div class="container">
    &copy; 2023 - serkanbilsel - <a asp-area="" asp-controller="Home" asp-action="Privacy"></a>
    </div> 
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
